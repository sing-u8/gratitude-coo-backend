		"@types/bcrypt": "^5.0.2",
		"@types/passport-jwt": "^4.0.1",
		"@types/winston": "^2.4.4",
		"bcrypt": "^5.1.1",
		"cache-manager": "^6.4.2",
		"class-transformer": "^0.5.1",
